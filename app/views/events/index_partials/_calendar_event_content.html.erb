<% if @events[week_num][day_num].size > 0 %>
  <% @events[week_num][day_num].each do |event| %>
    <p class="calendar-event-time"> <%= event.start_time.strftime("%I:%M %p") %> </p>
    <p class="calendar-event-presenter"> <%= get_event_presenter(event) %> </p>
    <p class="calendar-event-summary"> <%= get_event_summary(event) %> </p>
  <% end %>
<% else %>
  <% if Date.commercial(@date.year, week_num, day_num) >= Date.today %>
    <%= render partial: 'events/index_partials/schedule_event',
              locals: { modal_date: Date.commercial(@date.year, week_num, day_num) }%>
  <% else %>
    <p class="calendar-event-time"> No events </p>
  <% end %>
<% end %>